pipeline
{
	agent any
	stages
	{
		stage ('My first cloud formation template to launch ec2 instance.')
		{
			steps
			{
				withAWS(credentials: 'AWS-Ec2-S3-Access', region: 'us-west-2') {
				sh 'aws cloudformation create-stack --stack-name Cloud-Formation-EC2-create${BUILD_NUMBER} --template-body file://Json_Cloud_formation_EC2_instance.json'
				}
			}
		}
	}
}
