pipeline
{
	agent any
	stages
	{
		stage ('My first cloud formation template to launch ec2 instance.')
		{
			steps
			{
				withAWS(credentials: 'AWS-Ec2-user', role: 'Root user id') {
				sh 'aws cloudformation create-stack --stack-name Cloud-Formation-EC2-create${BUILD_NUMBER} --template-body file://Cloud_formation_EC2_Creation'
				}
			}
		}
	}
}
